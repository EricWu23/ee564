function result = avoid_rotor_slot(Qr,Qs,p)
% p = pole pairs
% Qr > 1.25Qs
if Qr == Qs || Qr == 0.5*Qs || Qr == 2*Qs || Qr == Qs + 2*p ||...
        Qr == Qs - 2*p || Qr == 2*Qs + 2*p || Qr == 2*Qs - 2*p ||...
        Qr == Qs + p || Qr == Qs - p || Qr == 0.5*Qs + p ||...
        Qr == 0.5*Qs - p
    result = 0;
elseif Qr == 6*p ||
Qr == 6pn +- 2p
Qr == 6pn +- 1
Qr == 6pn +- 2p +- 1
Qr == 6pn +- 2p

result = Qr*Qs*p;
end